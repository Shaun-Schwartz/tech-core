<h1>Organization</h1>

<table>
  <tbody>
    <tr>
      <th>Organization Name</th>
      <th>Published</th>
      <th>Action</th>
    </tr>

    <% @organizations.each do |organization| %>
      <tr>
        <td><%= organization.name %></td>
        <td><%= organization.published %></td>
        <td><%= link_to 'Show', organization %></td>
      </tr>
    <% end %>
  </tbody>
</table>
